<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

<title>{{page.title}}</title>

<!-- Bootstrap -->
<link rel="stylesheet" href=
{{#unless bootstrap}}
"https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
{{else}}
"/bootstrap/css/bootstrap.min.css">
{{/unless}}

<!-- Google Code Prettify -->
<link rel="stylesheet" href=
{{#unless prettify}}
"https://cdnjs.cloudflare.com/ajax/libs/prettify/r298/prettify.min.css">
{{else}}
"/google-code-prettify/prettify.min.css">
{{/unless}}

<!-- Highlight.js -->
<link rel="stylesheet" href=
{{#unless hljs}}
"https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/styles/default.min.css">
{{else}}
"/stylesheets/default.min.css">
{{/unless}}

<link href="/stylesheets/style.css" rel="stylesheet">
<link href="/stylesheets/basic.css" rel="stylesheet">
<link href="/stylesheets/custom.css" rel="stylesheet">

<!-- https://github.com/isagalaev/highlight.js/issues/304 -->
<style type="text/css">
    pre, pre code, .hljs {
        background: #f5f5f5;
        font-family: monospace;
    }
</style>